{
  "name": "sublime",
  "version": "0.4.0",
  "description": "A meta-package to bring some of Sublime Text's features to Atom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-sublime"
  },
  "keywords": [
    "sublime text",
    "sublimetext"
  ],
  "main": "./lib/sublime",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "atom-wrap-in-tag",
    "auto-update-plus",
    "browse",
    "build",
    "linter",
    "minimap",
    "multi-cursor",
    "set-syntax",
    "sublime-block-comment",
    "sublime-style-column-selection",
    "sublime-word-navigation",
    "sublimeify",
    "tag",
    "text-manipulation"
  ],
  "dependencies": {
    "atom-package-deps": "^4.3.1"
  }
}
