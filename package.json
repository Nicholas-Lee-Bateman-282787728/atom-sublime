{
  "name": "sublime",
  "version": "0.2.1",
  "description": "A meta-package to bring some of Sublime Text's features to Atom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-sublime"
  },
  "keywords": [
    "toolbar",
    "tool-bar",
    "package developer"
  ],
  "main": "./lib/sublime",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "browse",
    "build",
    "linter",
    "minimap",
    "multi-cursor",
    "set-syntax",
    "sublime-block-comment",
    "sublime-style-column-selection",
    "sublime-word-navigation",
    "tag"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1"
  }
}
